{
  "expo": {
    "name": "Men√º",
    "slug": "Menu",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/Menu-Icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Menu-Icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCF8E8"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Menu-Icons/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.menuapp.se"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Menu-Icons/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
